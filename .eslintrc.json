{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "indent": ["error", "tab"],
        "allowIndentationTabs" : 0,
        "arrow-spacing": 0,
        "no-tabs": 0,
        "space-before-blocks":0,
        "no-return-assign":0,
        "no-unused-vars":0,
        "disable-indent":0,
        "no-param-reassign":0,
        "no-restricted-syntax":0,
        "guard-for-in":0,
        "no-const-assign":0,
        "camelcase":2,
        "consistent-return":0,
        "no-useless-concat": 0
    }
}
